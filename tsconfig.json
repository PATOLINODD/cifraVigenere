{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "esnext",
      "esnext.asynciterable"
    ],
    "module": "preserve",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "noEmit": true,
      "strict": true,
      "noImplicitAny": true,
  },
  "include": ["scripts"]
}